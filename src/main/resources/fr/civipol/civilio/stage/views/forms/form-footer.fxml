<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.Cursor?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<ButtonBar prefHeight="45.0" prefWidth="294.0" xmlns="http://javafx.com/javafx/17"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.civipol.civilio.controller.FormFooterController">
    <buttons>
        <Button fx:id="btnFinish" defaultButton="true" mnemonicParsing="false" text="%forms.footer.actions.finish">
            <graphic>
                <FontIcon iconLiteral="fth-save"/>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="btnDiscard" cancelButton="true" mnemonicParsing="false" text="%forms.footer.actions.discard">
            <graphic>
                <FontIcon iconLiteral="fth-trash-2"/>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </buttons>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</ButtonBar>
